#include<iostream>
using namespace std;
int main() 
{
    int year_input1;
    char status_input1;
    
    cout<<"Xyris Zamoranos\n";
    cout<<"IT11S5\n";
    
    cout<< "Enter year Level: ";
    cin >> year_input1;
    
    cout<< "Enter status: ";
    cin >> status_input1;
    
    if (year_input1 == 1)
    {
        if (status_input1 == 'R' || status_input1 == 'r')
        {
            cout<<"The Student is First Year and Regular";
        }
        else if (status_input1 == 'I' || status_input1 == 'i')
        {
            cout<<"The Student is First Year and Irregular";
        }
        else
        {
            cout<<"The Student is First Year and Invalid";
        }
    }
    else if (year_input1 == 2)
    {
        if (status_input1 == 'R' || status_input1 == 'r')
        {
            cout<<"The Student is Second Year and Regular";
        }
        else if (status_input1 == 'I' || status_input1 == 'i')
        {
            cout<<"The Student is Second and Irregular";
        }
        else
        {
            cout<<"The Student is Second and Invalid";
        }
    }
    else if (year_input1 == 3)
    {
        if (status_input1 == 'R' || status_input1 == 'r')
        {
            cout<<"The Student is Third Year and Regular";
        }
        else if (status_input1 == 'I' || status_input1 == 'i')
        {
            cout<<"The Student is Third Year and Irregular";
        }
        else
        {
            cout<<"The Student is Third Year and Invalid";
        }
    }
    else if (year_input1 == 4)
    {
        if (status_input1 == 'R' || status_input1 == 'r')
        {
            cout<<"The Student is Fourth Year and Regular";
        }
        else if (status_input1 == 'I' || status_input1 == 'i')
        {
            cout<<"The Student is Fourth Year and Irregular";
        }
        else
        {
            cout<<"The Student is Fourth Year and Invalid";
        }
    }
    else
    {
        if (status_input1 == 'R' || status_input1 == 'r')
        {
            cout<<"The Student is Invalid and Regular";
        }
        else if (status_input1 == 'I' || status_input1 == 'i')
        {
            cout<<"The Student is Invalid and Irregular";
        }
        else
        {
            cout<<"The Student is Invalid and Invalid";
        }
    }
}
